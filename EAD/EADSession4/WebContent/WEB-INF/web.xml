<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<servlet-class>GET2018.EAD.EADSession4.Servlets.RegistrationServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/RegistrationServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ProfileServlet</servlet-name>
		<servlet-class>GET2018.EAD.EADSession4.Servlets.ProfileServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProfileServlet</servlet-name>
		<url-pattern>/ProfileServlet</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>AuthenticationFilter</filter-name>
		<filter-class>GET2018.EAD.EADSession4.Filters.AuthenticationFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<url-pattern>/ProfileServlet</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>EditInfoServlet</servlet-name>
		<servlet-class>GET2018.EAD.EADSession4.Servlets.EditInfoServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditInfoServlet</servlet-name>
		<url-pattern>/EditInfoServlet</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>SessionVerifyFilter</filter-name>
		<filter-class>GET2018.EAD.EADSession4.Filters.SessionVerifyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>SessionVerifyFilter</filter-name>
		<url-pattern>/EditInfoServlet</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>UpdateUserServlet</servlet-name>
		<servlet-class>GET2018.EAD.EADSession4.Servlets.UpdateUserServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateUserServlet</servlet-name>
		<url-pattern>/UpdateUserServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FriendsServlet</servlet-name>
		<servlet-class>GET2018.EAD.EADSession4.Servlets.FriendsServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FriendsServlet</servlet-name>
		<url-pattern>/FriendsServlet</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>SessionVerifyFilter</filter-name>
		<filter-class>GET2018.EAD.EADSession4.Filters.SessionVerifyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>SessionVerifyFilter</filter-name>
		<url-pattern>/FriendsServlet</url-pattern>
	</filter-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>